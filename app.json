{
  "expo": {
    "name": "FirstProject",
    "slug": "FirstProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "assets":["./assets/fonts/"],
    "fonts": {
      "Inconsolata": "./assets/fonts/Inconsolata.ttf",
      "Montserrat": "./assets/fonts/Montserrat.ttf"
    },

    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#06161C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
